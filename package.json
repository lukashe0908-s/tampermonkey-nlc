{
  "name": "react-userscripts",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "axios": "^1.6.8",
